@INPROCEEDINGS{Huang2008,
  author = {Huang, Chiachi and Jafar, S. A.},
  title = {Degrees of freedom of the MIMO interference channel with cooperation
	and cognition},
  booktitle = {Proc. IEEE International Symposium on Information Theory ISIT 2008},
  year = {2008},
  pages = {1348--1352},
  abstract = {In this paper, we explore the benefits, in the sense of total (sum
	rate) degrees of freedom (DOF), of cooperation and cognitive message
	sharing for a two-user multiple-input-multiple-output (MIMO) Gaussian
	interference channel with M1, M2 antennas at transmitters and N1,
	N2 antennas at receivers. For the case of cooperation (including
	cooperation at transmitters only, at receivers only, and at transmitters
	as well as receivers), the DOF is min{M1 + M2,N1 + N2, max(M1,N2),
	max(M2,N1)}, which is the same as the DOF of the channel without
	cooperation. For the case of cognitive message sharing, the DOF is
	min{M1+ M2,N1+N2, (1-1T2)((1-1R2)max(M1,N2)+1R2(M1+N2))+ 1T2(M1 +
	M2), (1 - 1T1)((1 - 1R1)max(M2,N1) + R1(M2 + N1)) + 1T1(M1 + M2)}
	where Ti = 1 (0) when transmitter i is (is not) a cognitive transmitter
	and Ri is defined in the same fashion. Our results show that while
	both techniques may increase the sum rate capacity of the MIMO interference
	channel, only cognitive message sharing can increase the DOF. We
	also find that it may be more beneficial for a user to have a cognitive
	transmitter than to have a cognitive receiver.},
